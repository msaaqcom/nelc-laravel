{
    "name": "msaaq/nelc-laravel",
    "description": "xAPI Integration with Saudi NELC (National Center for e-Learning) for your Laravel app",
    "keywords": [
        "xapi",
        "lrs-score",
        "nelc-laravel"
    ],
    "homepage": "https://github.com/msaaqcom/nelc-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Hussam Abd",
            "email": "hussam@msaaq.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "msaaq/nelc-xapi-php-sdk": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Msaaq\\NelcLaravel\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Msaaq\\NelcLaravel\\NelcServiceProvider"
            ],
            "aliases": {
                "Nelc": "Msaaq\\NelcLaravel\\Facades\\Nelc"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
